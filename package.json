{
	"name": "chat-nest-workspace",
	"private": true,
	"workspaces": [
		"packages/*",
		"apps/*"
	],  "scripts": {
        "build": "npm run build -ws",
        "dev": "npm run dev --workspace=demo",
    
        "build:sdk": "npm run build --workspace=chat-nest-sdk",
        "dev:sdk": "npm run dev --workspace=chat-nest-sdk",
    
        "build:server": "npm run build --workspace=chat-nest-server",
        "dev:server": "npm run dev --workspace=chat-nest-server",

        "build:core": "npm run build --workspace=chat-nest-core",
        "dev:core": "npm run dev --workspace=chat-nest-core"
      }    
}
